<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rdfs.framework.params.entity.SyParameter" table="sy_parameter" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="name" type="java.lang.String">
            <column name="param_name"/>
        </property>
        <property name="key" type="java.lang.String">
            <column name="param_key"/>
        </property>
        <property name="value" type="java.lang.String">
            <column name="param_value"></column>
        </property>
        <property name="group" type="java.lang.String">
            <column name="param_group" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"/>
        </property>
    </class>
</hibernate-mapping>
